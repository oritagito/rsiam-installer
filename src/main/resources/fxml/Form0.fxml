<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.PointLight?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" xmlns="http://javafx.com/javafx/8.0.141"
            fx:controller="ru.redsys.rsiam.installer.Form.Impl.Form0Controller">
    <PointLight color="CHARTREUSE" lightOn="true"/>
    <BorderPane prefHeight="500.0" prefWidth="700.0">
        <bottom>
            <ButtonBar prefHeight="40.0" prefWidth="277.0" BorderPane.alignment="CENTER">
                <buttons>
                    <Button fx:id="button3" cancelButton="true" onAction="#handleOnCloseButtonAction" prefHeight="25.0"
                            prefWidth="80.0" text="Отмена"/>
                    <Button fx:id="button2" disable="true" prefHeight="25.0" prefWidth="80.0" text="&lt; Назад"/>
                    <Button fx:id="button1" defaultButton="true" onAction="#handleOnNextButtonAction" prefHeight="25.0"
                            prefWidth="80.0" text="Далее &gt;"/>
                </buttons>
            </ButtonBar>
        </bottom>
        <top>
            <MenuBar prefHeight="0.0" prefWidth="700.0" BorderPane.alignment="CENTER">
                <menus>
                    <Menu mnemonicParsing="false" text="Справка">
                        <items>
                            <MenuItem mnemonicParsing="false" onAction="#handleOnHelpButtonAction" text="О программе"/>
                        </items>
                    </Menu>
                </menus>
            </MenuBar>
        </top>
        <center>
            <SplitPane dividerPositions="0.2" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0"
                       BorderPane.alignment="CENTER">
                <items>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                        <children>
                            <Text layoutX="14.0" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0"
                                  text="Вас приветствует мастер установки программы RS: Управление доступом"
                                  wrappingWidth="692.0">
                                <font>
                                    <Font size="19.0"/>
                                </font>
                            </Text>
                        </children>
                    </AnchorPane>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                        <children>
                            <Text layoutX="14.0" layoutY="291.0" strokeType="OUTSIDE" strokeWidth="0.0"
                                  text="ВНИМАНИЕ! Данная программа защищена законами об авторских правах и международными соглашениями. Незаконное воспроизведение или распространение данной программы или любой её части влечет гражданскую и уголовную ответственность."
                                  wrappingWidth="660.0"/>
                            <Text layoutX="14.0" layoutY="36.0" strokeType="OUTSIDE" strokeWidth="0.0"
                                  text="Мастер проведет Вас через все этапы устаноки программы RS: Управление доступом. Для продолжения нажмите кнопку &quot;Далее&quot;"
                                  wrappingWidth="488.0"/>
                        </children>
                    </AnchorPane>
                </items>
            </SplitPane>
        </center>
    </BorderPane>
</AnchorPane>
